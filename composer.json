{
  "name": "xoco70/laravel-tournaments",
  "description": "A Laravel 5.4+ Package that allows you to generate Tournaments tree",
  "homepage": "https://github.com/xoco70/laravel-tournaments",
  "license": "Apache License 2.0",
  "authors": [
    {
      "name": "juliatzin",
      "email": "xoco70@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Xoco70\\LaravelTournaments\\": "src"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Xoco70\\LaravelTournaments\\Tests\\": "tests"
    },
    "classmap": [
      "tests",
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Xoco70\\LaravelTournaments\\TournamentsServiceProvider"
      ]
    }
  },

  "require": {
    "php": ">=7.0.0",
    "phpunit/phpunit": "^6.0",
    "fzaninotto/faker": "1.*",
    "laravel/browser-kit-testing": "^1.0",
    "kalnoy/nestedset": "^4.2"
  },
  "scripts": {
    "test": "phpunit"
  },
  "require-dev": {
    "orchestra/testbench-browser-kit": "~3.5",
    "orchestra/database": "~3.1",
    "symfony/var-dumper": "~3.3.6"
  }
}
