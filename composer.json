{
  "name": "xoco70/laravel-tournaments",
  "description": "A Laravel 5.4+ Package that allows you to generate Tournaments tree",
  "homepage": "https://github.com/xoco70/laravel-tournaments",
  "license": "MIT",
  "authors": [
    {
      "name": "juliatzin",
      "email": "julien@cappiello.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Xoco70\\LaravelTournaments\\": "src"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Xoco70\\LaravelTournaments\\Tests\\": "tests"
    },
    "classmap": [
      "tests",
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Xoco70\\LaravelTournaments\\TournamentsServiceProvider"
      ]
    }
  },

  "require": {
    "php": ">=7.2.5",
    "kalnoy/nestedset": "^5.1"
  },
  "scripts": {
    "test": "phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "fzaninotto/faker": "1.*",
    "laravel/browser-kit-testing": "^6.0",
    "orchestra/testbench-browser-kit": "~5.0",
    "orchestra/database": "~5.1",
    "symfony/var-dumper": "~5.0"
  }
}
