{
  "name": "xoco70/kendo-tournaments",
  "description": "A Laravel 5.3+ Package that allows you to generate Tournaments tree, Direct Elimination, Round Robin, or Mixed",
  "type": "Project",
  "homepage": "https://github.com/xoco70/kendo-tournaments",
  "license": "Apache License 2.0",
  "authors": [
    {
      "name": "juliatzin",
      "email": "xoco70@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Xoco70\\KendoTournaments\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xoco70\\KendoTournaments\\Tests\\": "tests"
    },
    "4classmap": [
      "tests",
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "symfony/var-dumper": "~3.2.3",
    "phpunit/phpunit": "^5.7",
    "fzaninotto/faker": "1.*",
    "laravel/browser-kit-testing": "^1.0"

  },
  "scripts": {
    "test": "phpunit"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "orchestra/database": "~3.1"
  }
}
